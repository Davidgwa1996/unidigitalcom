{
  "name": "unidigitalcom-frontend",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.6.2",
    "chart.js": "^4.4.1",
    "dotenv": "^16.3.1",
    "formik": "^2.4.4",
    "framer-motion": "^10.18.0",
    "html-react-parser": "^5.1.8",
    "lucide-react": "^0.309.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-error-boundary": "^3.1.4",
    "react-feather": "^2.0.10",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.11.0",
    "react-markdown": "^9.0.1",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.20.1",
    "react-scripts": "5.0.0",
    "react-simple-maps": "^3.0.0",
    "recharts": "^2.10.0",
    "styled-components": "^6.1.1",
    "swiper": "^10.3.1",
    "web-vitals": "^2.1.4",
    "yup": "^1.3.3",
    "zod": "^3.22.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,css,md,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,css,md,json}\"",
    "clean": "rm -rf build node_modules/.cache",
    "clean:full": "rm -rf node_modules package-lock.json build",
    "clean:win": "Remove-Item -Recurse -Force node_modules; Remove-Item -Force package-lock.json",
    "fresh:win": "npm run clean:win && npm install",
    "fresh": "npm run clean:full && npm install",
    "dev": "react-scripts start",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "serve": "serve -s build",
    "docker:build": "docker build -t unidigital-frontend .",
    "docker:run": "docker run -p 3000:3000 unidigital-frontend",
    "heroku-postbuild": "npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "react-hooks/exhaustive-deps": "warn",
      "react/prop-types": "off",
      "react/react-in-jsx-scope": "off",
      "import/no-anonymous-default-export": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "ajv": "^8.12.0",
    "ajv-keywords": "^5.1.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "schema-utils": "^4.0.0",
    "tailwindcss": "^3.3.6"
  },
  "proxy": "http://localhost:5000",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "marketplace",
    "ecommerce",
    "cars",
    "electronics",
    "react",
    "ai",
    "modern"
  ],
  "author": "UniDigital Team",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/unidigitalcom-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/unidigitalcom-frontend/issues"
  },
  "homepage": "https://unidigital.com"
}
